@using eStartap.Models

@model Card;

@{
    ViewData["Title"] = "Delete";
}

<div>
    <div>
        <h4>
            Title:
        </h4>
        <p>
            @Model.Title
        </p>
    </div>
    <div>
        <h4>
            Date:
        </h4>
        <p>
            @Model.Date
        </p>
    </div>
    <div>
        <h4>
            Image:
        </h4>
        <p>
            <img src="~/assets/img/@Model.Image"/>
        </p>
    </div>
</div>


<form method="post" class="mt-4">
    <button type="submit" class="btn btn-danger">Delete</button>
</form>



